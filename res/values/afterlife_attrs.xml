<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Interval seekbar -->
    <declare-styleable name="IntervalSeekBar">
        <attr name="minI" format="float" />
        <attr name="maxI" format="float" />
        <attr name="defaultValuePure" format="float" />
        <attr name="digits" format="integer" />
    </declare-styleable>
    
	<attr name="cardType">
		<enum name="topLeft" value="0" />
		<enum name="topRight" value="1" />
		<enum name="middle" value="2" />
		<enum name="botLeft" value="3" />
		<enum name="botRight" value ="4" />
	</attr>
	<attr name="cardRight" format="boolean" />
	<attr name="cardBottom" format="boolean" />
	<attr name="showDividerRight" format="boolean" />
	<attr name="showDividerBottom" format="boolean" />

</resources>
